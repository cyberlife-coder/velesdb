{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Query Rate (QPS)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(velesdb_queries_total{status=\"success\"}[5m])",
          "legendFormat": "Success"
        },
        {
          "expr": "rate(velesdb_queries_total{status=\"error\"}[5m])",
          "legendFormat": "Errors"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Query Latency Percentiles",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(velesdb_query_duration_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(velesdb_query_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(velesdb_query_duration_seconds_bucket[5m]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Query Types Distribution",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "velesdb_queries_by_type",
          "legendFormat": "{{type}}"
        }
      ]
    },
    {
      "title": "Error Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 8 },
      "targets": [
        {
          "expr": "rate(velesdb_queries_total{status=\"error\"}[5m]) / rate(velesdb_queries_total[5m]) * 100",
          "legendFormat": "Error %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "title": "Active Connections",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 12 },
      "targets": [
        {
          "expr": "velesdb_active_connections",
          "legendFormat": "Connections"
        }
      ]
    },
    {
      "title": "Documents Total",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "velesdb_documents_total",
          "legendFormat": "Documents"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "Index Size",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "velesdb_index_size_bytes",
          "legendFormat": "Index Size"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "title": "Graph Traversal Stats",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 8 },
      "targets": [
        {
          "expr": "rate(velesdb_traversal_nodes_visited_total[5m])",
          "legendFormat": "Nodes/s"
        },
        {
          "expr": "rate(velesdb_traversal_edges_scanned_total[5m])",
          "legendFormat": "Edges/s"
        }
      ]
    },
    {
      "title": "Guard-Rails Triggered",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "increase(velesdb_limits_exceeded_total[1h])",
          "legendFormat": "{{limit_type}}",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      }
    },
    {
      "title": "Rate Limit Decisions",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "rate(velesdb_rate_limit_requests_total{decision=\"allowed\"}[5m])",
          "legendFormat": "Allowed"
        },
        {
          "expr": "rate(velesdb_rate_limit_requests_total{decision=\"rejected\"}[5m])",
          "legendFormat": "Rejected"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Max Traversal Depth",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 16 },
      "targets": [
        {
          "expr": "velesdb_traversal_max_depth",
          "legendFormat": "Max Depth"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 20,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 15 }
            ]
          }
        }
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["velesdb", "database", "vector", "graph"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "title": "VelesDB Overview",
  "uid": "velesdb-overview",
  "version": 1,
  "weekStart": ""
}
